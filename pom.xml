            <systemPath>${pom.basedir}/src/main/java/tw/momocraft/coreplus/extra/CMILib1.1.2.5.jar
            </systemPath>
            <version>LEGACY</version>
        </dependency>
        <!--Residence API-->
        <dependency>
            <groupId>me.Residence</groupId>
            <artifactId>Residence</artifactId>
            <scope>system</scope>
            <systemPath>${pom.basedir}/src/main/java/tw/momocraft/coreplus/extra/Residence5.0.1.3.jar
            </systemPath>
            <version>LEGACY</version>
        </dependency>
        <!--ItemJoin API-->
        <dependency>
            <groupId>me.RockinChaos.itemjoin</groupId>
            <artifactId>ItemJoin</artifactId>
            <version>5.2.0-RELEASE</version>
            <scope>provided</scope>
        </dependency>
        <!--AuthMe API-->
        <dependency>
            <groupId>fr.xephi</groupId>
            <artifactId>authme</artifactId>
            <version>5.6.0-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
        <!--PvPManager API-->
        <dependency>
            <groupId>me.PvPManager</groupId>
            <artifactId>PvPManager</artifactId>
            <scope>system</scope>
            <systemPath>${pom.basedir}/src/main/java/tw/momocraft/coreplus/extra/PvPManager-3.16.7.jar
            </systemPath>
            <version>LEGACY</version>
        </dependency>
        <!--Multiverse-Core API-->
        <dependency>
            <groupId>me.Multiverse-Core</groupId>
            <artifactId>Multiverse-Core</artifactId>
            <scope>system</scope>
            <systemPath>${pom.basedir}/src/main/java/tw/momocraft/coreplus/extra/Multiverse-Core-4.3.1.jar
            </systemPath>
            <version>LEGACY</version>
        </dependency>
        <!--MythicMobs API-->
        <dependency>
            <groupId>me.MythicMobs</groupId>
            <artifactId>MythicMobs</artifactId>
            <scope>system</scope>
            <systemPath>${pom.basedir}/src/main/java/tw/momocraft/coreplus/extra/MythicMobs-5.1.0-SNAPSHOT.jar
            </systemPath>
